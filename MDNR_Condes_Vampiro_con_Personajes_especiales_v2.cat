          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="9c6b-4170-b4ea-a0b4" type="max"/>
          </constraints>
          <profiles>
            <profile id="950b-4f07-156b-6c1e" name="Astrología impía" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
                <characteristic name="Propiedades mágicas" typeId="e33b-a1fc-9f43-c88d">Un solo uso. El Vampiro puede repetir un único dado de una tirada para lanzar un hechizo (esto puede evitar una disfunción o provocar una Fuerza Irresistible).</characteristic>
              </characteristics>
            </profile>
          </profiles>
