        </selectionEntry>
        <selectionEntry id="f520-ae34-bdf4-5374" name="Estandarte siniestro" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="f110-b55e-ff6b-a095" type="max"/>
          </constraints>
          <profiles>
            <profile id="aac1-2d99-2bb0-af2c" name="Estandarte siniestro" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
                <characteristic name="Propiedades mágicas" typeId="e33b-a1fc-9f43-c88d">El portador y la unidad en la que esté causan Miedo.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="75.0"/>
