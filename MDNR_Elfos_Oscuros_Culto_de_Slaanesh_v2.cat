            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="7497-21f2-3ccb-d932" type="max"/>
          </constraints>
          <profiles>
            <profile id="61a7-e59d-c50a-33b3" name="Escudo rúnico del Caos" hidden="false" typeId="7895-caab-ea85-11a6" typeName="Armadura">
              <characteristics>
