            <characteristic name="L" typeId="458e-bb4b-f7c9-e0b3">7(9)</characteristic>
            <characteristic name="TSA" typeId="2532-2940-06ec-7a6e">4+</characteristic>
            <characteristic name="Tipo" typeId="4a99-e6a0-4022-a833">Infantería, Pielverde, Goblin, Goblin Nocturno, Personaje especial</characteristic>
