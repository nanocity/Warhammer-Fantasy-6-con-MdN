      <profiles>
        <profile id="1cc9-b554-c581-9a99" name="Kurt Helborg" hidden="false" typeId="1025-8460-b30f-58db" typeName="Modelo">
          <characteristics>
            <characteristic name="M" typeId="2d56-9cad-9242-37bd"/>
