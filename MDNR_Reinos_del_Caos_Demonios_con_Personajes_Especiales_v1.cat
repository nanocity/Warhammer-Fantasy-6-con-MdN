        <selectionEntry id="024f-264a-5e2c-0f63" name="Niebla Mortal (10+)" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="1743-14eb-4a8f-d6fe" type="max"/>
