            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="cd8e-c8a5-7293-07fa" type="max"/>
          </constraints>
          <profiles>
            <profile id="6c28-e559-0c53-b172" name="Forma proteica" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">Sólo miniaturas de tipo Tzeentch. El personaje gana la regla Regeneración (4+).</characteristic>
