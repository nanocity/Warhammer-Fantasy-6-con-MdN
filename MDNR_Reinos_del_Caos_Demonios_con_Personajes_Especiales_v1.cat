          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="45.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="84f5-0bc2-12ff-cd6c" name="El Ojo Dorado de Tzeentch" hidden="false" collective="false" import="true" type="upgrade">
