        <selectionEntry id="9cbc-9fbf-d5ab-6862" name="Báculo del hechicero" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="ea10-077e-84e8-6013" type="max"/>
