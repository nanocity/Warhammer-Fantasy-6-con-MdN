        <profile id="1d34-275d-66a0-fb65" name="Tanque de vapor" hidden="false" typeId="1025-8460-b30f-58db" typeName="Modelo">
          <characteristics>
            <characteristic name="M" typeId="2d56-9cad-9242-37bd">*</characteristic>
