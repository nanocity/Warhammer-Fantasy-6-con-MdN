          </constraints>
          <profiles>
            <profile id="f0ed-7e5a-174e-cad1" name="Estandarte del Templo" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
                <characteristic name="Propiedades mágicas" typeId="e33b-a1fc-9f43-c88d">El portador y su unidad obtienen resistencia a la magia (2).</characteristic>
              </characteristics>
