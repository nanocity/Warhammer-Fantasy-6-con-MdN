            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="2b27-7066-d999-3041" type="max"/>
          </constraints>
          <profiles>
            <profile id="19b6-8a7b-67c9-3f6d" name="Grito ensordecedor" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">Sólo miniaturas de tipo Khorne. En el turno en que el personaje haya cargado, las miniaturas enemigas en contacto peana con peana con el personaje tienen un -1 a las tiradas para impactarle.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="15.0"/>
