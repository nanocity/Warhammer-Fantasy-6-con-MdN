            <profile id="689a-4b99-5fb1-b496" name="Poder de Khorne" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">El Demonio tiene F+1. </characteristic>
