            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="0d78-6867-be12-7f55" type="max"/>
          </constraints>
          <profiles>
            <profile id="48bc-eb7c-fb24-5679" name="Cuerpo de serpiente" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">Sólo miniaturas de tipo Slaanesh. El personaje obtiene M+5 e I+1. Los personajes con Cuerpo de serpiente no pueden tener Pezuñas.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="20.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="85e5-c865-f42f-99c8" name="Fauces extensibles" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="5186-757d-2087-59ac" type="max"/>
