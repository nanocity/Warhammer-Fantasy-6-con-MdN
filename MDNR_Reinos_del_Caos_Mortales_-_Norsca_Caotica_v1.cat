          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="25.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="785c-ed45-fa90-b5db" name="Esplendor diabólico" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="2624-c496-0d43-1125" type="max"/>
          </constraints>
          <profiles>
            <profile id="d5f2-48cf-220a-2f6f" name="Esplendor diabólico" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">Tanto el Demonio como la unidad en la que esté incluido pueden efectuar su Aura Demoníaca (Tirada de salvación especial de 5+) contra cualquier tipo de ataque, incluidos ataques mágicos y hechizos.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="20.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="8fee-1254-5c1b-90ad" name="Ídolo demoníaco" hidden="false" collective="false" import="true" type="upgrade">
