        </selectionEntry>
        <selectionEntry id="55a1-8959-c2fe-244e" name="Aura de gloria infernal" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="70b1-4d54-393a-0369" type="max"/>
          </constraints>
          <profiles>
            <profile id="b5b0-aa10-161b-b67f" name="Aura de gloria infernal" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">Toda miniatura a 15cm o menos de una o más Auras de Gloria Infernal sufrirán una herida menos debido a la regla Inestabilidad demoníaca. Por ejemplo, si pierden el combate de 3, sólo recibirán 2 heridas. Por supuesto, se aplica también al propio Demonio con Aura de Gloria Infernal.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="40.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="6b49-3308-6037-ba11" name="Avidez de almas" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="5e98-1269-e4b2-f8b2" type="max"/>
