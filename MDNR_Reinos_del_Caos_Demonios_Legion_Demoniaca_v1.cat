      <profiles>
        <profile id="ea43-f52f-b66a-defc" name="Be&apos;lakor" hidden="false" typeId="1025-8460-b30f-58db" typeName="Modelo">
          <characteristics>
            <characteristic name="M" typeId="2d56-9cad-9242-37bd">15</characteristic>
