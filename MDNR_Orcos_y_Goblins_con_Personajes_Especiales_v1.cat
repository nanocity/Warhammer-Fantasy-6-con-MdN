            <infoLink id="6e65-3a68-6cee-b7e0" name="Objetivo grande" hidden="false" targetId="9e28-dc38-0f21-77f6" type="rule"/>
            <infoLink id="8c2b-8814-f05d-73a4" name="Terror" hidden="false" targetId="31ab-3ff3-e34f-baac" type="rule"/>
