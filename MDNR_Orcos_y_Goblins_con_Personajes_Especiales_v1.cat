  <selectionEntries>
    <selectionEntry id="c76f-cc67-773f-41b7" name="Grimgor Piel&apos;ierro (Personajes Especiales, 1 Comandante + 1 Héroe + 1 Unidad Especial)" hidden="false" collective="false" import="true" type="model">
