<catalogue id="a59d-b03e-d6db-953c" name="MDNR Orcos y Goblins con Personajes Especiales" revision="7" battleScribeVersion="2.03" authorName="Largo" authorContact="largonelli@gmail.com" authorUrl="https://eltemplodemyrmidia.wordpress.com/" library="false" gameSystemId="fd97-5cfe-d46c-03c6" gameSystemRevision="4" xmlns="http://www.battlescribe.net/schema/catalogueSchema">
