                                      </infoLinks>
                                      <costs>
                                        <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="45.0"/>
                                      </costs>
                                    </selectionEntry>
                                    <selectionEntry id="70f3-94a5-ae16-ea25" name="Honor Puro de corazón (obligatorio en al menos un personaje, no repetible)" hidden="false" collective="false" import="true" type="upgrade">
                                      <constraints>
                                        <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="9aad-0f02-0851-f97e" type="max"/>
