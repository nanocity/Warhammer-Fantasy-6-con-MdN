            <characteristic name="H" typeId="a41f-60f6-2336-73ab">4</characteristic>
            <characteristic name="I" typeId="fdd9-0cd1-90d5-1b6f">7</characteristic>
            <characteristic name="A" typeId="6b2b-37c4-6774-748b">5</characteristic>
