        <selectionEntry id="42fe-0864-56da-37f1" name="Báculo del hechicero" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="28fc-4baf-8dd6-5593" type="max"/>
