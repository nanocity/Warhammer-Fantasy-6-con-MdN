    <selectionEntry id="fa0d-6297-7a9a-234d" name="Lorenzo Lupo, Príncipe Mercader de Luccini (Personaje Especial, 1 Comandante + 1 Héroe)" hidden="false" collective="false" import="true" type="model">
      <constraints>
