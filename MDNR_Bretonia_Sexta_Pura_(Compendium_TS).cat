          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="1bd9-fc0e-3bf1-97a0" type="max"/>
          </constraints>
          <profiles>
            <profile id="75f3-a6cb-7f14-fe63" name="Virtud de la heroicidad" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">Si el caballero lucha contra un Objetivo grande y no está usando arma mágica, tendrá Golpe letal contra esa miniatura (esta Virtud prevalece sobre la regla que dice que Golpe letal no afecta a Objetivos grandes).</characteristic>
