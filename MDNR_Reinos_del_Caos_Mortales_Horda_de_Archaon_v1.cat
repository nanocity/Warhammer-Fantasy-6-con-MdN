    <selectionEntry id="f73a-59c8-503c-a573" name="Caudillo centauro (Sólo puedes incluir 0-1 Caudillos Centauros por cada unidad de Centauros)" hidden="false" collective="false" import="true" type="model">
      <profiles>
        <profile id="558b-d21b-6ae3-2f76" name="Caudillo centauro" hidden="false" typeId="1025-8460-b30f-58db" typeName="Modelo">
          <characteristics>
            <characteristic name="M" typeId="2d56-9cad-9242-37bd">20</characteristic>
            <characteristic name="HA" typeId="c87b-b820-6a47-1354">5</characteristic>
