    <selectionEntryGroup id="05b0-a9c3-c461-d0d1" name="Mutaciones del caos" hidden="false" collective="false" import="true">
      <selectionEntries>
        <selectionEntry id="7931-0a48-1714-3c60" name="Alas" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="8da6-d545-aa44-125d" type="max"/>
          </constraints>
          <profiles>
            <profile id="9bc2-3176-4ba9-8981" name="Alas" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">Sólo miniaturas de tipo Infantería. El personaje puede Volar y no puede llevar Montura (ni siquiera un Carro).</characteristic>
              </characteristics>
