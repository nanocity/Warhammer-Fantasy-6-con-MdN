          <profiles>
            <profile id="ed6d-84f5-ab8d-50a8" name="Veneno negro" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
                <characteristic name="Propiedades mágicas" typeId="e33b-a1fc-9f43-c88d">El portador gana Ataques envenenados. (Las miniaturas Inmunes a ataques envenenados siguen siéndolo).</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="10.0"/>
          </costs>
        </selectionEntry>
      </selectionEntries>
