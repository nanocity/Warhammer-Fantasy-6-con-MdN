            <characteristic name="Tipo" typeId="4a99-e6a0-4022-a833">Caballería, Humano, Caos.</characteristic>
            <characteristic name="Potencia" typeId="6bbb-9823-b0cd-5705">2</characteristic>
          </characteristics>
        </profile>
