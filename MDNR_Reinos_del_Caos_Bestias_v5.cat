    <selectionEntry id="8f65-9267-d30a-6525" name="Caudillo centauro (Sólo puedes incluir 0-1 Caudillos Centauros por cada unidad de Centauros)" hidden="false" collective="false" import="true" type="model">
      <profiles>
        <profile id="bcc7-cf78-3e8d-2f63" name="Caudillo centauro" hidden="false" typeId="1025-8460-b30f-58db" typeName="Modelo">
          <characteristics>
            <characteristic name="M" typeId="2d56-9cad-9242-37bd">20</characteristic>
