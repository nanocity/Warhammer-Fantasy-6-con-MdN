          <profiles>
            <profile id="a686-ce02-0bd6-09d2" name="Collar de Khorne" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">Sólo miniaturas de tipo Khorne. Resistencia a la Magia (2).</characteristic>
              </characteristics>
            </profile>
