          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="20.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="2c92-f131-9b21-5d18" name="Ídolo demoníaco" hidden="false" collective="false" import="true" type="upgrade">
