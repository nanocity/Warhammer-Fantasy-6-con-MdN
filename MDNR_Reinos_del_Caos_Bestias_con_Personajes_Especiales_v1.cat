          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="10.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="7113-0afc-7210-827e" name="Señor de los mortales" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="506a-0b91-6b38-7dfb" type="max"/>
          </constraints>
          <profiles>
            <profile id="3509-938a-f70b-b464" name="Señor de los mortales" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">A efectos de creación de ejército el Demonio se considera Mortal (por lo que para saber las unidades básicas, especiales y singulares tienes que mirar el libro de Guerreros del Caos). No se puede equipar a un Demonio con Ídolo Demoníaco y Señor de los Mortales a la vez. </characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="5.0"/>
          </costs>
        </selectionEntry>
