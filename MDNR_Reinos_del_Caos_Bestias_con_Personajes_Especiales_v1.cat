    <selectionEntryGroup id="e96e-bc47-dcb4-e44e" name="Recompensas demoníacas Khorne" hidden="false" collective="false" import="true">
      <selectionEntries>
        <selectionEntry id="329f-895a-0511-cad1" name="Collar de Khorne" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="53d1-f616-7419-11a8" type="max"/>
          </constraints>
          <profiles>
            <profile id="1611-53cd-cfad-7550" name="Collar de Khorne" hidden="false" typeId="7e35-c20a-e9e9-6dad" typeName="Talismán">
              <characteristics>
                <characteristic name="Protección" typeId="9f41-c15e-1f5f-9d01">El Demonio, así como cualquier unidad a la que se le haya unido, gana Resistencia a la magia (2).</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="40.0"/>
