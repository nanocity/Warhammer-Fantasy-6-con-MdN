    <selectionEntry id="70f1-d08a-30c9-8e41" name="Coloso Necrofex" hidden="false" collective="false" import="true" type="unit">
      <profiles>
        <profile id="bec1-2e8b-9303-77dd" name="Coloso Necrofex" hidden="false" typeId="1025-8460-b30f-58db" typeName="Modelo">
          <characteristics>
            <characteristic name="M" typeId="2d56-9cad-9242-37bd"/>
            <characteristic name="HA" typeId="c87b-b820-6a47-1354"/>
            <characteristic name="HP" typeId="8ab7-c800-6644-f5ec"/>
